{
    "manifest_version": 3,
    "name": "Word Meaning Popup",
    "version": "1.0",
    "description": "Get the meaning of selected words in a popup.",
    "permissions": [
      "activeTab"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon":"extIcon.png"
    }
    
  }